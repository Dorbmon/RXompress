<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.10.3 -->
<interface>
  <!-- interface-name rxompress.ui -->
  <requires lib="gtk" version="4.6"/>
  <object class="GtkWindow" id="RXompress">
    <property name="height-request">5</property>
    <child>
      <object class="GtkBox" id="WholeContent">
        <property name="hexpand">True</property>
        <property name="opacity">0.4</property>
        <property name="orientation">vertical</property>
        <property name="vexpand">True</property>
        <child>
          <object class="GtkBox" id="BarBox">
            <property name="baseline-position">bottom</property>
            <property name="hexpand">True</property>
            <property name="overflow">hidden</property>
            <property name="spacing">4</property>
            <child>
              <object class="GtkButton" id="AboutButton">
                <property name="label">About</property>
              </object>
            </child>
            <child>
              <object class="GtkTreeView"/>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="ContentBox">
            <property name="hexpand">True</property>
            <property name="vexpand">True</property>
            <child>
              <object class="GtkPaned">
                <property name="hexpand">True</property>
                <child>
                  <object class="GtkScrolledWindow" id="DirListWin">
                    <property name="child">
                      <object class="GtkTreeView" id="DirList"/>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkScrolledWindow" id="FileListWin">
                    <property name="child">
                      <object class="GtkTreeView" id="FileList"/>
                    </property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
